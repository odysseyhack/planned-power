{
  "name": "planned-power",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start:ganache": "npm run copy-contract-build && ng serve --proxy-config proxy.conf.js -c ganache-dev",
    "start:vmware": "npm run copy-contract-build && ng serve --proxy-config proxy.conf.js -c vmware-dev --host 0.0.0.0",
    "build": "npm run copy-contract-build && ng build --prod",
    "deploy:dev": "truffle migrate --reset --network=development && npm run copy-contract-build",
    "copy-contract-build": "rm -rf ./src/assets/contracts && cp -r ./build/contracts ./src/assets/contracts",
    "test": "npm run copy-contract-build && ng test  --watch=false",
    "test:travis": "npm run copy-contract-build && ng test --watch=false",
    "truffle:test:ganache": "truffle test ./test/Orders.js && truffle test ./test/Order.js && truffle test ./test/Upgrade.js",
    "truffle:test:vmware": "truffle test ./test/Orders.js --network=vmware && truffle test ./test/Order.js --network=vmware && truffle test ./test/Upgrade.js --network=vmware",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node patch.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^7.2.2",
    "@angular/core": "^7.2.2",
    "@angular/forms": "^7.2.2",
    "@angular/http": "^7.2.2",
    "@angular/platform-browser": "^7.2.2",
    "@angular/platform-browser-dynamic": "^7.2.2",
    "@angular/router": "^7.2.2",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^4.1.0",
    "@ionic/lab": "^1.0.24",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "truffle-contract": "^4.0.11",
    "web3": "0.20.7",
    "zone.js": "~0.8.29",
    "zos-lib": "1.4.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.12.3",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/core": "~7.2.3",
    "@angular-devkit/schematics": "~7.2.3",
    "@angular/cli": "~7.3.1",
    "@angular/compiler": "~7.2.2",
    "@angular/compiler-cli": "~7.2.2",
    "@angular/language-service": "~7.2.2",
    "@ionic/angular-toolkit": "~1.4.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.14.2",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.11.0",
    "protractor": "~5.4.0",
    "ts-node": "~8.0.0",
    "tslint": "~5.12.0",
    "typescript": "~3.1.6"
  },
  "description": "An Ionic project"
}
